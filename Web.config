<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>

	<system.webServer>
		<staticContent>
			<remove fileExtension=".geojson" />
			<remove fileExtension=".zip" />
			<mimeMap fileExtension=".geojson" mimeType="application/json" />
			<mimeMap fileExtension=".zip" mimeType="application/zip" />
		</staticContent>
	</system.webServer>
</configuration>